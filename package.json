{
  "name": "stock",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.25.1",
    "antd": "^4.16.13",
    "axios": "^0.21.4",
    "react": "^17.0.2",
    "react-dnd": "^14.0.3",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.1",
    "umi": "^3.5.17",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.14",
    "@umijs/plugin-qiankun": "^2.32.2",
    "@umijs/preset-react": "^1.8.23",
    "@umijs/test": "^3.5.17",
    "babel-plugin-styled-components": "^1.13.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "yorkie": "^2.0.0"
  }
}
